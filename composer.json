{
    "name": "czim/laravel-dataobject",
    "description": "Basic validatable standardized data object.",
    "keywords": [
        "data object",
        "standardization",
        "validate",
        "validation"
    ],
    "homepage": "https://github.com/czim/laravel-dataobject",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "opensource@czim.nl",
            "homepage": "https://github.com/czim",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "nunomaduro/larastan": "^2.5",
        "orchestra/testbench": "^8.0",
        "phpstan/phpstan-mockery": "^1.1",
        "phpstan/phpstan-phpunit": "^1.1",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Czim\\DataObject\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\DataObject\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
